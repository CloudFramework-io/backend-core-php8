                "cols": "3",
                "allow_empty": true,
                "tab": "development"
            },
            "Status": {
                "name": "$namespace:cloudframework,cfo;project;variables;status",
                "type": "select",
                "defaultvalue": "pending",
                "values": [
                    {
                        "id": "pending",
                        "value": "Pending"
                    },
                    {
                        "id": "in-progress",
                        "value": "In Progress"
                    },
                    {
                        "id": "blocked",
                        "value": "Blocked"
                    },
                    {
                        "id": "in-qa",
                        "value": "In QA"
                    },
                    {
                        "id": "closed",
                        "value": "Closed"
                    }
                ]
            },
            "Open": {
                "name": "$namespace:cloudframework,cfo;project;variables;open",
                "internal_replacement": "1",
                "value": "1",
                "conditional_value": {
                    "Status == closed or canceled": {
                        "value": "0",
                        "condition": [
                            "{{Status}}",
                            "equals",
                            [
                                "closed",
                                "canceled"
                            ]
                        ]
                    }
                }
            },
            "CompanyId": {
                "name": "$namespace:cloudframework,cfo;project;variables;company_id",
                "internal_replacement_if_found": "1",
                "internal_source": "datastore",
                "entity": "CloudFrameWorkProjectsEntries",
                "linked_field": "KeyName",
                "linked_value": "{{ProjectId}}",
                "value": "{{CompanyId}}"
            },
            "Service": {
                "name": "$namespace:cloudframework,cfo;project;variables;invoice_title"
            },
            "TimeSpent": {
                "name": "$namespace:cloudframework,cfo;project;variables;time_spent",
                "disabled": "1"
            },
            "TimeEstimated": {
                "name": "$namespace:cloudframework,cfo;project;variables;time_estimated",
                "allow_empty": true,
                "defaultvalue": "0"
            },
            "Title": {
                "name": "$namespace:cloudframework,cfo;project;variables;title",
                "header": {
                    "title": "Milestone Description"
                },
                "cols": "3"
            },
            "Description": {
                "name": "$namespace:cloudframework,cfo;project;variables;description",
                "type": "html",
                "defaultvalue": "<p>Your milestone description</p>"
            },
            "Docs": {
                "name": "$namespace:cloudframework,cfo;project;variables;docs",
                "type": "server_documents",
                "bucket": "gs://cloudframework.io/documents/files/{{Platform:spacename}}",
                "cf_bucket": "KeyName",
                "wrap": true,
                "allow_empty": true,
                "limit_chars": {
                    "max_chars": "20"
                }
            },
            "JSON": {
                "name": "$namespace:cloudframework,cfo;project;variables;structured_data",
                "type": "json",
                "defaultvalue": "{}"
            },
            "WebApps": {
                "name": "Apps Relacionadas",
                "type": "multiselect",
                "external_values": "ds",
                "entity": "CloudFrameWorkDevDocumentationForWebApps",
                "fields": "KeyName,Title",
                "linked_field": "KeyName",
                "limit": "1000",
                "cols": "3",
                "allow_empty": true,
                "tab": "development"
            },
            "APIs": {
                "name": "APIs Relacionadas",
                "type": "multiselect",
                "external_values": "ds",
                "entity": "CloudFrameWorkDevDocumentationForAPIs",
                "fields": "KeyName,ExtraPath",
                "linked_field": "KeyName",
                "limit": "1000",
                "allow_empty": true,
                "cols": "3",
                "tab": "development"
            },
            "VirtualTaks": {
                "name": "$namespace:cloudframework,cfo;project;variables;tasks",
                "type": "virtual",
                "external_info": "datastore",
                "entity": "CloudFrameWorkProjectsTasks",
                "linked_field": "MilestoneId",
                "field_value": "KeyId",
                "order": "DateInitTask DESC",
                "fields": "*",
                "summary_line": "[{{DateInitTask}}/{{DateDeadline}}] - ({{PlayerId}})[{{Priority}}:{{Status}}] {{Title}}",
                "external_new_entity_url": "https://api.cloudframework.io/core/cfo/cfi/CloudFrameWorkProjectsTasks/insert?filter_MilestoneId={{KeyId}}&filter_ProjectId={{ProjectId}}&filter_CompanyId={{CompanyId}}"
            }
        }
